{
  "name": "starter-postgres",
  "version": "1.0.0",
  "description": "A starter template for express.js projects using objection.js and passport",
  "main": "src/index.js",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "knex": "^0.15.2",
    "objection": "^1.4.0",
    "objection-db-errors": "^1.0.0",
    "objection-password": "^2.0.0",
    "objection-unique": "^1.0.1",
    "pg": "^7.6.1"
  },
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "supertest": "^3.3.0"
  },
  "scripts": {
    "start": "node src/index.js",
    "test": "jest --runInBand --coverage --forceExit",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "migrate": "knex migrate:latest --knexfile=src/knexfile.js",
    "rollback-migrations": "knex migrate:rollback --knexfile=src/knexfile.js",
    "generate-dotenv": "./src/utils/generateEnvFiles.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/programma-group/starter-postgres.git"
  },
  "keywords": [
    "starter",
    "objection",
    "express"
  ],
